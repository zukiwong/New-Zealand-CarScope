# Server Configuration
PORT=3000
NODE_ENV=development

# Trade Me API Configuration (只需要 Consumer Key/Secret 做签名)
TRADEME_API_BASE_URL=https://api.trademe.co.nz/v1
TRADEME_CONSUMER_KEY=your_consumer_key_here
TRADEME_CONSUMER_SECRET=your_consumer_secret_here

# Cache Configuration
CACHE_TTL=300
CACHE_CHECK_PERIOD=60

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=100

# CORS
CORS_ORIGIN=http://localhost:5173

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log
